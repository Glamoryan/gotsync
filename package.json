{
  "name": "gotsync-workspace",
  "version": "1.0.0",
  "description": "Workspace for gotsync CLI and related packages",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "dev": "npm run dev --workspaces",
    "clean": "npm run clean --workspaces",
    "cli": "node packages/cli/dist/index.js"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/node": "^20.10.0",
    "@types/swagger-parser": "^4.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=7"
  },
  "keywords": [
    "cli",
    "gotsync",
    "typescript",
    "go",
    "code-generation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@apidevtools/swagger-parser": "^12.0.0",
    "ejs": "^3.1.10",
    "openapi-types": "^12.1.3"
  }
}
